
    <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
        <h4 class="modal-title">{{ t("file_history", "File History") }} ({{ file.originalFilename }})</h4>
    </div>

    <div class="modal-body">
        <div class="row">
            <div class="col-md-12 account-history-table">
                <table id="historyTable" class="table table-striped table-sm">
                    <thead>
                    <tr>
                        <th style="width: 30%">
                            Description:
                        </th>
                        <th>
                            More Information:
                        </th>
                    </tr>
                    </thead>
                    <tbody>

                    {% for userActionLog in userActionLogs %}
                    <tr>
                        <td>
                            <strong>{{ userActionLog.message }}</strong><br/>
                            on {{ CoreHelper.formatDate(userActionLog.date_created) }}<br/>
                            by {{ userActionLog.getActionUsername() }}
                        </td>
                        <td>
                            {{ userActionLog.formatParamsForUserView()|raw }}
                        </td>
                    </tr>
                    {% endfor %}

                    </tbody>
                </table>
            </div>
        </div>
    </div>

    <div class="modal-footer">
        <button type="button" class="btn btn-default" data-dismiss="modal">{{ t("close", "close")|title }}</button>
    </div>
